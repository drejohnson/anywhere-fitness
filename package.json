{
  "name": "anywhere-fitness",
  "version": "0.1.0",
  "description": "Fitness app built with Svelte + Faunadb + GraphQL",
  "scripts": {
    "start": "vercel dev --listen 3000",
    "build": "snowpack build",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod",
    "clean": "rm -rf build/*",
    "snowpack:dev": "snowpack dev --open none --port ${PORT:-3000} # Use Vercel-specified port, or default to 8080",
    "vercel": "vercel",
    "vercel:runAppDev": "npm run snowpack:dev # `vercel dev` runs this per Vercel project settings, sets $PORT env var",
    "vercel:runAppBuild": "npm run build # Vercel runs this per Vercel project settings (on website GUI)",
    "test": "web-test-runner \"src/**/*.test.ts\""
  },
  "keywords": [
    "svelte",
    "tailwindcss",
    "faunadb",
    "graphql",
    "jamstack",
    "serverless",
    "snowpack",
    "rollup"
  ],
  "license": "MIT",
  "author": {
    "name": "Dre Johnson",
    "email": "dre@phreshr.com",
    "url": "https://drejohnson.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drejohnson/anywhere-fitness"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.11.1",
    "@hapi/iron": "^6.0.0",
    "@urql/svelte": "^1.1.3",
    "cookie": "^0.4.1",
    "faunadb": "^4.0.0",
    "firebase": "^8.1.1",
    "firebase-admin": "^9.4.1",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "nodemailer": "^6.4.16",
    "rxfire": "^4.0.0",
    "rxjs": "^7.0.0-beta.8",
    "svelte": "^3.29.7",
    "svelte-navigator": "^3.1.3",
    "xstate": "^4.15.1"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.3.4",
    "@snowpack/plugin-dotenv": "^2.0.4",
    "@snowpack/plugin-optimize": "^0.2.9",
    "@snowpack/plugin-run-script": "^2.2.0",
    "@snowpack/plugin-svelte": "^3.3.0",
    "@snowpack/plugin-typescript": "^1.1.0",
    "@snowpack/web-test-runner-plugin": "^0.1.4",
    "@testing-library/svelte": "^3.0.0",
    "@types/chai": "^4.2.13",
    "@types/cookie": "^0.4.0",
    "@types/googlemaps": "^3.40.5",
    "@types/nodemailer": "^6.4.0",
    "@types/snowpack-env": "^2.3.2",
    "@vercel/node": "^1.8.5",
    "@web/test-runner": "^0.9.7",
    "autoprefixer": "^10.0.1",
    "chai": "^4.2.0",
    "ncp": "^2.0.0",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "postcss-load-config": "^3.0.0",
    "postcss-nested": "^5.0.1",
    "prettier": "^2.2.0",
    "prettier-plugin-svelte": "^1.4.1",
    "snowpack": "^2.17.1",
    "snowpack-plugin-rollup-bundle": "^0.3.3",
    "svelte-check": "^1.1.16",
    "svelte-preprocess": "^4.6.1",
    "tailwindcss": "^1.9.6",
    "typescript": "^4.1.2"
  }
}
